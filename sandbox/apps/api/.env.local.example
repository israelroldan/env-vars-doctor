# =============================================================================
# API App Environment Variables
# =============================================================================

# [required] [prompt] JWT secret for signing tokens
JWT_SECRET=

# [optional] [default:3001] Port to run the API server
PORT=3001

# [optional] [default:100] Rate limit per minute
RATE_LIMIT=100

# [local-only] [default:true] Enable request logging locally
REQUEST_LOGGING=true
